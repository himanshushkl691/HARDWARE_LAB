module MUX2X1_16BIT_TB;
   wire[15:0] out;
   reg [15:0] in0,in1;
   reg 	      s0;
   MUX2X1_16BIT mux0(out,s0,in0,in1);
   initial
     begin
	$dumpfile("MUX2X1_16BIT_TB.vcd");
	$dumpvars(0,MUX2X1_16BIT_TB);
	#100;
	s0 = 1;
	in0 = 16;
	in1 = 14;
	#10;
	s0 = 0;
	in0 = 16;
	in1 = 14;
     end // initial begin
endmodule // MUX2X1_16BIT_TB
